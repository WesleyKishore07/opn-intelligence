DEBUG=false
ENVIRONMENT=staging
API_V1_STR=/api/v1

# Database
DATABASE_URL=postgresql://user:password@db-staging:5432/opn_staging
ASYNC_DATABASE_URL=postgresql+asyncpg://user:password@db-staging:5432/opn_staging

# Redis
REDIS_URL=redis://redis-staging:6379/0

# JWT
JWT_SECRET_KEY=your-secret-key-staging
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Service-specific
SHIPMENT_RATE_LIMIT=5000
QUOTE_CACHE_TTL=3600